<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  	<display-name>Archetype Created Web Application</display-name>
  	
  	<!-- profile servlet -->
  	<servlet>
  		<servlet-name>profile</servlet-name>
  		<servlet-class>app.servlet.ProfileServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
  		<servlet-name>profile</servlet-name>
  		<url-pattern>/profile</url-pattern>
  	</servlet-mapping>
  
  	<!-- Project Servlet -->
  	<servlet>
  		<servlet-name>projects</servlet-name>
  		<servlet-class>app.servlet.ProjectServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
  		<servlet-name>projects</servlet-name>
  		<url-pattern>/project</url-pattern>
  	</servlet-mapping>
  	
  	<!-- More Info Servlet -->
  	
  <servlet>
  	<servlet-name>moreinfo</servlet-name>
  	<servlet-class>app.servlet.MoreInfo</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>moreinfo</servlet-name>
  	<url-pattern>/moreinfo</url-pattern>
  </servlet-mapping>
</web-app>
